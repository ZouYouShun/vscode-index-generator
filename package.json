{
  "name": "index-generator",
  "displayName": "index-generator",
  "publisher": "declanzou",
  "description": "Create index.ts(js) for folder.",
  "repository": "https://github.com/ZouYouShun/vscode-index-generator",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:index-generator.createIndex",
    "onCommand:index-generator.createIndexOnlyTarget"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "index-generator.createIndex",
        "title": "Index-Generator: Create index all file and folder with this target folder and sub folder."
      },
      {
        "command": "index-generator.createIndexOnlyTarget",
        "title": "Index-Generator: Create only target file index file with this target folder."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "@types/ncp": "^2.0.1",
    "@types/chokidar": "^2.1.3",
    "@types/eslint": "^6.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/prettier": "^1.18.2",
    "@types/shelljs": "^0.8.5",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "chokidar": "^2.1.5",
    "commander": "^2.20.0",
    "eslint": "^6.3.0",
    "fs-extra": "^8.1.0",
    "ignore": "^5.1.2",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "ncp": "^2.0.0"
  }
}
